# Firebase Configuration for Local Development
# Copy this file to .env.local and replace with your actual values
# .env.local is automatically ignored by git for security

VITE_FIREBASE_CONFIG='{
  "apiKey": "your-actual-api-key-here",
  "authDomain": "simulateai-research.firebaseapp.com",
  "projectId": "simulateai-research",
  "storageBucket": "simulateai-research.firebasestorage.app",
  "messagingSenderId": "your-actual-sender-id",
  "appId": "your-actual-app-id",
  "measurementId": "your-actual-measurement-id"
}'

# Other environment variables
VITE_RECAPTCHA_SITE_KEY=your-recaptcha-key
VITE_VAPID_KEY=your-vapid-key
